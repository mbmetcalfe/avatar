;; Macro to try and target all things passed.
;; "hit" is synonymous to "kill", but you can create a "hit" alias to do something else
/def autotarget = \
    /if ({atarg} == 1) \
        /while ({#}) \
            /if ({mudLag} < 3) \
                /send hit "%{1}"%;\
                /shift%;\
            /endif%;\
        /done%;\
    /endif

